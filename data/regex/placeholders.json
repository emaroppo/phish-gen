{
    "emails": {
        "placeholder": "EMAIL",
        "regex": [
            "(?:[a-z0-9!#$%&'*+/=?^_`{|}~-]+(?:\\.[a-z0-9!#$%&'*+/=?^_`{|}~-]+)*|(?:[\\x01-\\x08\\x0b\\x0c\\x0e-\\x1f\\x21\\x23-\\x5b\\x5d-\\x7f]|\\\\[\\x01-\\x09\\x0b\\x0c\\x0e-\\x7f])*)@(?:(?:[a-z0-9](?:[a-z0-9-]*[a-z0-9])?\\.)+[a-z0-9](?:[a-z0-9-]*[a-z0-9])?|\\[(?:(?:(2(5[0-5]|[0-4][0-9])|1[0-9][0-9]|[1-9]?[0-9]))\\.){3}(?:(2(5[0-5]|[0-4][0-9])|1[0-9][0-9]|[1-9]?[0-9])|[a-z0-9-]*[a-z0-9]:(?:[\\x01-\\x08\\x0b\\x0c\\x0e-\\x1f\\x21-\\x5a\\x53-\\x7f]|\\\\[\\x01-\\x09\\x0b\\x0c\\x0e-\\x7f])+)\\])"
        ]
    },
    "urls": {
        "placeholder": "URL",
        "regex": [
            "(https?:\\/\\/\\S+)",
            "(www\\.\\S+(?:\\.\\w+)+\\b)"
        ]
    },
    "attachments": {
        "placeholder": "ATTACHMENT",
        "regex": [
            "<<\\s+File:(.*?)>>",
            "<<File\\s+Attachment:\\s+(.*?)>>",
            "\\(See attached file:(.*?)\\)",
            "<<((?:.*?)\\.(?:[A-z0-9]{1,4}))>>",
            "- ((?:.*?)\\.(?:[A-z0-9]{1,4}))"
        ]
    },
    "dates": {
        "placeholder": "DATE",
        "regex": [
            "(?:(?:Mon(?:day)?|Tue(?:sday)?|Wed(?:nesday)?|Thu(?:rsday)?|Fri(?:day)?|Sat(?:urday)?|Sun(?:day)?)\\.?,?\\s+)?(?:Jan(?:uary)?|Feb(?:ruary)?|Mar(?:ch)?|Apr(?:il)?|May|Jun(?:e)?|Jul(?:y)?|Aug(?:ust)?|Sep(?:tember)?|Oct(?:ober)?|Nov(?:ember)?|Dec(?:ember)?)\\.?(?:,?\\s|\\s?-\\s?|\\s?/\\s?)(?:\\b(?:3[01]|[1-2]\\d|0?[1-9])(?:\\s?(?:st|nd|rd|th))?\\b)(?:,?\\s|\\s?-\\s?|\\s?/\\s?)(?:19|20|)?\\d{2}",
            "(?:(?:Mon(?:day)?|Tue(?:sday)?|Wed(?:nesday)?|Thu(?:rsday)?|Fri(?:day)?|Sat(?:urday)?|Sun(?:day)?)\\.?,?\\s+)?(?:\\b(?:3[01]|[1-2]\\d|0?[1-9])(?:\\s?(?:st|nd|rd|th))?\\b)(?:(?:,?\\s|\\s?-\\s?|\\s?/\\s?)|\\s+of\\s+)(?:Jan(?:uary)?|Feb(?:ruary)?|Mar(?:ch)?|Apr(?:il)?|May|Jun(?:e)?|Jul(?:y)?|Aug(?:ust)?|Sep(?:tember)?|Oct(?:ober)?|Nov(?:ember)?|Dec(?:ember)?)\\.?(?:,?\\s|\\s?-\\s?|\\s?/\\s?)(?:19|20|)?\\d{2}",
            "(?:\\b(?:3[01]|[1-2]\\d|0?[1-9])(?:\\s?(?:st|nd|rd|th))?\\b)(?:(?:,?\\s|\\s?-\\s?|\\s?/\\s?)|\\s+of\\s+)(?:Jan(?:uary)?|Feb(?:ruary)?|Mar(?:ch)?|Apr(?:il)?|May|Jun(?:e)?|Jul(?:y)?|Aug(?:ust)?|Sep(?:tember)?|Oct(?:ober)?|Nov(?:ember)?|Dec(?:ember)?)\\.?",
            "(?:Jan(?:uary)?|Feb(?:ruary)?|Mar(?:ch)?|Apr(?:il)?|May|Jun(?:e)?|Jul(?:y)?|Aug(?:ust)?|Sep(?:tember)?|Oct(?:ober)?|Nov(?:ember)?|Dec(?:ember)?)\\.?(?:,?\\s|\\s?-\\s?|\\s?/\\s?)(?:\\b(?:3[01]|[1-2]\\d|0?[1-9])(?:\\s?(?:st|nd|rd|th))?\\b)",
            "(?:(?:Jan(?:uary)?|Feb(?:ruary)?|Mar(?:ch)?|Apr(?:il)?|May|Jun(?:e)?|Jul(?:y)?|Aug(?:ust)?|Sep(?:tember)?|Oct(?:ober)?|Nov(?:ember)?|Dec(?:ember)?)\\.?(?:,?\\s|\\s?-\\s?|\\s?/\\s?)(?:19|20|)?\\d{2})"
        ]
    },
    "phone_numbers": {
        "placeholder": "PHONE",
        "regex": [
            "(?:(?:\\+\\d{1,2} )?\\d{3}-\\d{3}-\\d{4})(?:\\s+(?:ext\\.?|x|extension)\\s?(?:\\d+-?\\d*))?",
            "(?:(?:\\+\\d{1,2} )?\\(\\d{3}\\) \\d{3}-\\d{4})(?:\\s+(?:ext\\.?|x|extension)\\s?(?:\\d+-?\\d*))?",
            "(?:(?:\\+\\d{1,2} )?\\d{3}\\.\\d{3}\\.\\d{4})(?:\\s+(?:ext\\.?|x|extension)\\s?(?:\\d+-?\\d*))?",
            "(?:(?:\\+\\d{1,2} )?\\d{3} \\d{3} \\d{4})(?:\\s+(?:ext\\.?|x|extension)\\s?(?:\\d+-?\\d*))?",
            "(?:ext\\.?|x|extension)\\s?(?:\\d+-?\\d*)"
        ]
    }
}